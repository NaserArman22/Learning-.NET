
@{
    ViewBag.Title = "Reference";
}
@model valuePassing.Models.Reference[]
<h2>Reference</h2>
<table class="table table-bordered">
    <tr>
        <td>Designation</td>
        <td>Name
        <td>Email</td>

    </tr>
    @for (int i = 0; i < Model.Length; i++)
    {
        <tr>
            <td>@Model[i].designation</td>
            <td>@Model[i].name</td>
            <td>@Model[i].email</td>

        </tr>
    }


